# Environment variables for HBDrop
# Copy this file to .env and update with your actual values

# Encryption master key for session data
# Generate a secure key: openssl rand -base64 32
ENCRYPTION_MASTER_KEY=your_generated_encryption_key_here

# PostgreSQL password
# Generate a secure password for production
POSTGRES_PASSWORD=your_secure_postgres_password_here

# Giphy API key for GIF search functionality
# Get your free API key at: https://developers.giphy.com/
GIPHY_API_KEY=your_giphy_api_key_here
